@model ContactModel@{    ViewBag.Title = "title";    Layout = "_Layout";}<div class="card">    <div class="card-header">        Szczegóły    </div>    <div class="card-body">        <h5 class="card-title">@Model.FirstName @Model.LastName</h5>    </div>    <ul class="list-group list-group-flush">        <li class="list-group-item">Email: @Model.Email</li>        <li class="list-group-item">Telefon: @Model.PhoneNumber</li>        <li class="list-group-item">Data urodzin: @Model.BirthDate</li>    </ul>    <div class="card-body">        <a class="btn btn-primary" asp-action="Index" class="card-link">Powrót do listy</a>    </div></div>